<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="true"/>

        <service
            class="Tests\Sylius\AdyenPlugin\Unit\Mock\NullMessageBus"
            id="tests.sylius_adyen.null_message_bus"
        />

        <service
            class="Symfony\Component\Messenger\TraceableMessageBus"
            id="tests.sylius_adyen.message_bus"
        >
            <argument type="service" id="tests.sylius_adyen.null_message_bus"/>
        </service>

        <service
            class="Sylius\AdyenPlugin\Bus\Dispatcher"
            id="tests.sylius_adyen.bus.dispatcher"
        >
            <argument type="service" id="tests.sylius_adyen.message_bus"/>
            <argument type="service" id="sylius_adyen.bus.payment_command_factory"/>
        </service>

    </services>
</container>